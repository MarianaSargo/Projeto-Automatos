digraph "Aceita sequencias pares de zeros" {
	graph [rankdir=LR]
	node [shape=circle]
	edge [arrowhead=vee arrowsize=1]
	Start [label="" shape=none]
	B
	A
	C [shape=doublecircle]
	Start -> A
	A -> A [label=1]
	A -> B [label=0]
	B -> B [label=1]
	B -> C [label=0]
	C -> B [label=0]
	C -> C [label=1]
}
